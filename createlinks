#!/usr/bin/perl -w

use esmith::Build::CreateLinks qw(:all);

my $event = 'nethserver-delegated-panel-update';

event_actions($event, 'initialize-default-databases' => '00');

foreach my $event (qw ( nethserver-delegated-panel-update nethserver-delegated-panel-save)) {
templates2events("/usr/share/nethesis/NethServer/Authorization/DelegatedPanel.json",  $event);
}
